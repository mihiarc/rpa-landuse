# API Keys (at least one required)
OPENAI_API_KEY=
ANTHROPIC_API_KEY=

# Model Configuration
LANDUSE_MODEL=gpt-4o-mini
TEMPERATURE=0.1
MAX_TOKENS=4000

# Database Configuration
LANDUSE_DB_PATH=data/processed/landuse_analytics.duckdb
LANDUSE_MAX_QUERY_ROWS=1000
LANDUSE_DEFAULT_DISPLAY_LIMIT=50

# Agent Execution Limits
LANDUSE_MAX_ITERATIONS=5        # Max tool calls before stopping (default: 5)
LANDUSE_MAX_EXECUTION_TIME=120  # Max seconds for query execution (default: 120)

# Logging Configuration
LOG_LEVEL=INFO
ENABLE_LOGGING=true

# Rate Limiting Configuration
ENABLE_RATE_LIMITING=true
LANDUSE_RATE_LIMIT_CALLS=60    # Max calls per time window (default: 60)
LANDUSE_RATE_LIMIT_WINDOW=60   # Time window in seconds (default: 60)